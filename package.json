{
  "name": "agri-flow-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo without test now && exit 0",
    "wts": "tsc --watch",
    "start": "NODE_PATH=./dist node dist/server.js",
    "build": "tsc --project tsconfig.json && npx tsc-alias -p tsconfig.json",
    "dev": "nodemon src/server.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepare": "husky install",
    "generateUml": "npx prisma-uml ./prisma/schema.prisma -o png -f ./database/uml/uml.png",
    "generateUml:svg": "npx prisma-uml ./prisma/schema.prisma -o svg -f ./database/uml/uml.png"
  },
  "_moduleAliases": {
    "@middleware": "src/middleware",
    "@config": "src/configs",
    "@controller": "src/controllers",
    "@service": "src/services",
    "@util": "src/utils",
    "@helper": "src/helpers",
    "@model": "src/models",
    "@type": "src/@types",
    "@mock": "src/mocks"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@prisma/engines": "^3.12.0-31.ae010d3ee58f28c22599b1e5fbd1697a82240550",
    "@supabase/storage-js": "^2.0.0",
    "ajv": "^8.11.0",
    "atob-lite": "^2.0.0",
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-rate-limit": "^6.6.0",
    "is-base64": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "redis": "^4.3.1",
    "sharp": "^0.31.1",
    "svg-to-img": "^2.0.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/atob-lite": "^2.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/is-base64": "^1.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "17.0.18",
    "@types/nodemailer": "^6.4.6",
    "@types/sharp": "^0.31.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.15",
    "prisma": "^4.0.0",
    "ts-alias": "^0.0.5",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "4.5.5",
    "typescript-module-alias": "^2.2.9"
  },
